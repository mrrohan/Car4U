@model Car4U.Models.ExtraModelType

@{
    ViewBag.Title = "Details";
}



<div>
    <h4>Extra Model Type</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Description)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Description)
        </dd>

    </dl>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <div class="buttonbackoffice">
                <button class="submit" type="button" onclick="location.href='@Url.Action("Edit", "ExtraModelTypes", new { id = Model.ID })'">Editar</button>
                <button class="submit" type="button" onclick="location.href='@Url.Action("Delete", "ExtraModelTypes", new { id = Model.ID })'">Eliminar</button>
                <button class="submit" type="button" onclick="location.href='@Url.Action("Index", "ExtraModelTypes")'">Voltar</button>
            </div>
        </div>
    </div>
</div>

