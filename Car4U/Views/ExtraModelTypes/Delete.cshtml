@model Car4U.Models.ExtraModelType
@{
    Layout = "~/Views/Shared/_Layout2.cshtml";
}
@{
    ViewBag.Title = "Delete";
}


<div class="page-header">
    <h4>Tem a certeza que pretende apagar este extra model?</h4>
</div>
<div class="container">
    

    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Description)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Description)
        </dd>

    </dl>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">

            <div class="buttonbackoffice">
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    <div class="form-actions">
                        <button class="submit icon-Delete" type="submit" value="Delete"> Apagar </button>
                    </div>
                }
                <button class="submit icon-back" type="button" onclick="location.href='@Url.Action("Index", "ExtraModelTypes")'"> Voltar </button>
            </div>
        </div>
    </div>    
</div>
