@model Car4U.Models.Brand
@{
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

@{
    ViewBag.Title = "Delete";
}



<h2>Tem a certeza que pretende apagar esta Marca?</h2>
<div>


    <dl class="dl-horizontal">
        <dt>
            <h4> @Html.DisplayNameFor(model => model.Description) : </h4>
        </dt>

        <dd>
            <h4> @Html.DisplayFor(model => model.Description) </h4>
        </dd>

    </dl>

    <div class="form-group">
        <div class="buttonbackoffice">

            @using (Html.BeginForm())
            {

                @Html.AntiForgeryToken()

                <div class="form-actions">
                    <button class="submit  icon-Delete" type="submit" value="Delete"> Apagar </button>
                </div>
            }
            <button class="submit  icon-back" type="button" onclick="location.href='@Url.Action("IndexModel", "Brands")'"> Voltar </button> 
        </div>

    </div>
</div>
