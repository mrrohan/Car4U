@model Car4U.Models.Category

@{
    ViewBag.Title = "Details";
}
<div class="page-header">
    <h2>Detalhes da Categoria</h2>
</div>
<div class="container">
    <div class=" form-horizontal">
        <div class="form-group">
            <div class=" col-md-10">
                <table class=" table tableheader" style="width:200px">
                    <tr><th></th><th></th></tr>
                    <tr>
                        <td>@*@Html.DisplayNameFor(model => model.CategoryName)*@ Nome de Categoria</td>
                        <td>@Html.DisplayFor(model => model.CategoryName)</td>
                    </tr>
                    <tr>
                        <td>@*@Html.DisplayNameFor(model => model.Price)*@ Preço </td>
                        <td>@Html.DisplayFor(model => model.Price)</td>
                    </tr>
                    <tr>
                        <td>@*@Html.DisplayNameFor(model => model.Warranty)*@ Garantia</td>
                        <td>@Html.DisplayFor(model => model.Warranty)</td>
                    </tr>
                </table>


                <div class="buttonbackoffice">
                    <button class="submit icon-edit" type="button" onclick="location.href='@Url.Action("Edit", "Categories", new { id = Model.ID })'"> Editar </button>
                    <button class="submit  icon-Delete" type="button" onclick="location.href='@Url.Action("Delete", "Categories", new { id = Model.ID })'"> Eliminar </button>
                    <button class="submit  icon-back" type="button" onclick="location.href='@Url.Action("PrivateIndex", "Categories")'">  Voltar </button>
                </div>
            </div>
        </div>
    </div>
</div>