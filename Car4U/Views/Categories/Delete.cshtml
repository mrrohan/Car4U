@model Car4U.Models.Category

@{
    ViewBag.Title = "Delete";
}



<h2>Tem a certeza que pretende eliminar esta categoria?</h2>
<div>
    <h4>Category</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.CategoryName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CategoryName)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Price)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Price)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Warranty)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Warranty)
        </dd>

    </dl>

    <div class="form-group">
        <div class="buttonbackoffice">

            @using (Html.BeginForm())
            {

                @Html.AntiForgeryToken()

                <div class="form-actions">

                    <button class="submit  icon-Delete" type="submit" value="Delete"> Apagar </button>

                </div>
            }

            <button class="submit  icon-back" type="button" onclick="location.href='@Url.Action("PrivateIndex", "ExtraModels")'"> Voltar </button>

        </div>

    </div>
</div>
